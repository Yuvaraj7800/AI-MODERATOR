{
  "manifest_version": 3,
  "name": "AIModerator",
  "description": "AI Moderator",
  "action": {
    "default_icon": "assets/icon_48.png"
  },
  "icons": {
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "version": "1.0",
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "scripts/content_script.js"
    ]
  }],
  "permissions": [
    "tabs",
    "storage"
  ]
}